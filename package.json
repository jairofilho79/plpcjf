{
  "name": "plpcjf",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "postinstall": "node scripts/copy-pdfjs-viewer.mjs",
    "dev": "vite dev",
    "dev:cloudflare": "npm run build && wrangler pages dev .svelte-kit/cloudflare",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "generate-icons": "node scripts/generate-icons.mjs",
    "generate-offline-packages": "node scripts/generate-offline-packages.mjs"
  },
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "^4.3.1",
    "@sveltejs/kit": "^2.0.0",
    "fflate": "^0.8.2",
    "lucide-svelte": "^0.436.0",
    "pdfjs-dist": "^4.8.69"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.19",
    "pdfkit": "^0.15.0",
    "postcss": "^8.4.38",
    "sharp": "^0.34.4",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.3",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.11",
    "wrangler": "^4.45.3",
    "yazl": "^2.5.1"
  }
